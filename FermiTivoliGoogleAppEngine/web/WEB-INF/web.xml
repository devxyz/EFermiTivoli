<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


    <servlet>
        <servlet-name>UpdateCircolariServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.UpdateCircolariServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InvalidateServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.InvalidateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadCircolareServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.DownloadCircolareServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrintCircolariServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.PrintCircolariServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.ResetServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCircolariServletV2</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.UpdateCircolariServletV2</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>JSonCircolariServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.JSonCircolariServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadExternalDataServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.v2.LoadExternalDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClearAllServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.v2.ClearAllServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StatServlet</servlet-name>
        <servlet-class>it.gov.fermitivoli.server.servlet.v2.StatServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>InvalidateServlet</servlet-name>
        <url-pattern>/InvalidateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSonCircolariServlet</servlet-name>
        <url-pattern>/JSonCircolariServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetServlet</servlet-name>
        <url-pattern>/ResetServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrintCircolariServlet</servlet-name>
        <url-pattern>/PrintCircolariServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadCircolareServlet</servlet-name>
        <url-pattern>/DownloadCircolareServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateCircolariServlet</servlet-name>
        <url-pattern>/UpdateCircolariServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateCircolariServletV2</servlet-name>
        <url-pattern>/UpdateCircolariServletV2</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoadExternalDataServlet</servlet-name>
        <url-pattern>/LoadExternalDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClearAllServlet</servlet-name>
        <url-pattern>/ClearAllServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StatServlet</servlet-name>
        <url-pattern>/StatServlet</url-pattern>
    </servlet-mapping>


    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>it.gov.fermitivoli.server.ofy.OfyHelper</listener-class>
    </listener>


</web-app>